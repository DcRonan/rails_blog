<h1> All Tags </h1>

<ul id="tags">
  <% @tags.each do |tag| %>
    <li>
      <%= link_to tag.name, tag_path(tag), class: 'tag_name', id: "tag_#{tag.id}" %>
      <%= link_to "delete", tag_path(tag), method: :delete, data: {confirm: "Confirm that you want to delete?"} %>
    </li>
  <% end %>
</ul>
<br>
<%= link_to "<< Back to Articles List", articles_path %>